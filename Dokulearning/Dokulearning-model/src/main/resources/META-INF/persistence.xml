<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Dokulearning-model">
		<jta-data-source>jdbc/tiw</jta-data-source>
		<class>es.uc3m.tiw.model.dominios.Curso</class>
		<class>es.uc3m.tiw.model.dominios.Imagenes</class>
		<class>es.uc3m.tiw.model.dominios.Leccion</class>
		<class>es.uc3m.tiw.model.dominios.Material</class>
		<class>es.uc3m.tiw.model.dominios.Pruebas</class>
		<class>es.uc3m.tiw.model.dominios.ResultadosPruebas</class>
		<class>es.uc3m.tiw.model.dominios.Matriculados</class>
		<class>es.uc3m.tiw.model.dominios.Seccion</class>
		<class>es.uc3m.tiw.model.dominios.Usuarios</class>
		<class>es.uc3m.tiw.model.dominios.CreadorCurso</class>
		<properties>
			<property name="javax.persistence.schema-generation.database.action" value="drop-and-create"/>
		</properties>
	</persistence-unit>
</persistence>